<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.demo.domain.ApplyEntity" table="apply" schema="kjb">
        <id name="aId">
            <column name="a_id" sql-type="int(11)"/>
        </id>
        <property name="username">
            <column name="username" sql-type="varchar(255)"/>
        </property>
        <property name="amount">
            <column name="amount" sql-type="float" precision="-1"/>
        </property>
        <property name="rate">
            <column name="rate" sql-type="float" precision="-1"/>
        </property>
        <property name="repaytime">
            <column name="repaytime" sql-type="int(11)"/>
        </property>
        <property name="creditorName">
            <column name="creditor_name" sql-type="varchar(255)"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int(255)"/>
        </property>
    </class>
</hibernate-mapping>